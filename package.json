{
  "name": "@tim-smart/openapi-gen",
  "version": "0.3.2",
  "description": "Generate Effect http clients from OpenAPI specs",
  "packageManager": "pnpm@9.10.0",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "test": "pnpm coverage",
    "clean": "rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/openapi-gen.git"
  },
  "keywords": [],
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@effect/cli": "^0.48.9",
    "@effect/platform": "^0.69.9",
    "@effect/platform-node": "^0.64.10",
    "@types/node": "^22.8.2",
    "effect": "^3.10.4",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "yaml": "^2.6.0"
  }
}
