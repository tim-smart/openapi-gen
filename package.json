{
  "name": "@tim-smart/openapi-gen",
  "version": "0.3.22",
  "description": "Generate Effect http clients from OpenAPI specs",
  "packageManager": "pnpm@9.10.0",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "test": "pnpm coverage",
    "clean": "rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/openapi-gen.git"
  },
  "keywords": [],
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@effect/cli": "^0.61.3",
    "@effect/platform": "^0.82.3",
    "@effect/platform-node": "^0.81.0",
    "@types/node": "^22.15.18",
    "@types/swagger2openapi": "^7.0.4",
    "effect": "^3.15.2",
    "prettier": "^3.5.3",
    "swagger2openapi": "^7.0.8",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "yaml": "^2.8.0"
  }
}
